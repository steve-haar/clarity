{
  "name": "clarity",
  "version": "13.0.1",
  "engines": {
    "yarn": "please-use-npm",
    "node": ">=17.0.0",
    "npm": ">=7.10.0"
  },
  "scripts": {
    "build": "npm-run-all _build:angular _build:ui _build:schematics _build:demo",
    "build:ci": "npm-run-all clean lint build test public-api:check",
    "clean": "rimraf dist",
    "commit": "cz -s",
    "lint": "npm-run-all _lint:code _lint:styles _lint:format",
    "lint:fix": "npm-run-all _lint:code:fix _lint:styles:fix _lint:format:fix",
    "lint:changed": "npm-run-all _lint:code:changed _lint:styles:changed _lint:format:changed",
    "lint:changed:fix": "npm-run-all _lint:code:changed:fix _lint:styles:changed:fix _lint:format:changed:fix",
    "prepare": "husky install",
    "preview": "semantic-release -p \"@semantic-release/commit-analyzer\" \"@semantic-release/release-notes-generator\" --dry-run",
    "public-api:check": "api-extractor run",
    "public-api:update": "cpy ./dist/golden/clarity.api.md ./golden/",
    "start": "ng serve demo",
    "test": "npm-run-all _test:angular _test:schematics",
    "test:watch": "npm run --silent _test:angular -- --watch",
    "_build:angular": "ng build clr-angular --configuration production",
    "_build:angular:watch": "npm run --silent build:angular -- --watch",
    "_build:demo": "ng build demo --configuration production",
    "_build:schematics": "npm-run-all _build:schematics_clr:compile _build:schematics_clr:copy _build:schematics_test:compile _build:schematics_test:copy",
    "_build:schematics_clr:compile": "tsc -p ./projects/schematics/tsconfig.json",
    "_build:schematics_clr:copy": "cpy \"**/*.json\" ./../../../dist/clr-angular/schematics --cwd=./projects/schematics/src --parents",
    "_build:schematics_test:compile": "tsc -p ./projects/schematics/tsconfig.test.json",
    "_build:schematics_test:copy": "cpy \"**/*.json\" ./../../../dist/schematics --cwd=./projects/schematics/src --parents",
    "_build:ui:css": "sass ./projects/ui/src:./dist/clr-ui --precision=8",
    "_build:ui:prefix": "postcss ./dist/clr-ui/clr-ui.css ./dist/clr-ui/clr-ui-dark.css --use autoprefixer -d ./dist/clr-ui",
    "_build:ui:optimize": "csso ./dist/clr-ui/clr-ui.css -o ./dist/clr-ui/clr-ui.min.css -s file --no-restructure & csso ./dist/clr-ui/clr-ui-dark.css -o ./dist/clr-ui/clr-ui-dark.min.css -s file --no-restructure",
    "_build:ui:src": "cpy \"**/*.scss\" ./../../dist/clr-ui --cwd=./projects/angular --parents",
    "_build:ui:package": "cpy ./projects/ui/package.json ./projects/ui/README.md ./dist/clr-ui",
    "_build:ui": "npm-run-all _build:ui:css _build:ui:prefix _build:ui:optimize _build:ui:src _build:ui:package",
    "_exec_changed": "npx -q run-func scripts/getChangedFiles.js main",
    "_lint:code": "eslint . --ext .js,.json,.ts",
    "_lint:code:fix": "npm run --silent _lint:code -- --fix",
    "_lint:code:changed": "npm run --silent _exec_changed -- \"**/*.{js,json,ts}\" eslint",
    "_lint:code:changed:fix": "npm run --silent _lint:code:changed -- --fix",
    "_lint:format": "prettier . -c --loglevel warn",
    "_lint:format:fix": "npm run --silent _lint:format -- -w",
    "_lint:format:changed": "npm run --silent _exec_changed -- \"**\" prettier -c --loglevel warn --ignore-unknown",
    "_lint:format:changed:fix": "npm run --silent _lint:format:changed -- -w",
    "_lint:styles": "stylelint \"**/*.{css,scss,sass}\"",
    "_lint:styles:fix": "npm run --silent _lint:styles -- --fix",
    "_lint:styles:changed": "npm run --silent _exec_changed -- \"**/*.{css,scss,sass}\" stylelint",
    "_lint:styles:changed:fix": "npm run --silent _lint:styles:changed -- --fix",
    "_test:angular": "ng test clr-angular --source-map=false --watch=false",
    "_test:schematics": "jasmine \"./dist/schematics/**/*.spec.js\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@cds/angular": "^6.0.0-next.0",
    "@cds/core": "^6.0.0-next.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@amanda-mitchell/semantic-release-npm-multiple": "2.17.0",
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular-devkit/core": "~13.0.1",
    "@angular-devkit/schematics": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@commitlint/cz-commitlint": "^16.1.0",
    "@microsoft/api-extractor": "7.19.4",
    "@schematics/angular": "~13.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "commitizen": "^4.2.4",
    "commitlint": "^16.1.0",
    "cpy-cli": "^3.1.0",
    "csso-cli": "^3.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-license-header": "^0.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jasmine": "~3.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-expect": "^5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-parallel": "^0.3.1",
    "lint-staged": "^12.3.2",
    "ng-packagr": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "run-func": "^2.0.0",
    "semantic-release": "19.0.2",
    "semantic-release-slack-bot": "3.5.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
